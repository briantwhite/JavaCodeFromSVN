	<Gene2_SameChrAsGene1>1.0</Gene2_SameChrAsGene1>
	<Gene2_MinRfToGene1>0.5</Gene2_MinRfToGene1>
	<Gene2_MaxRfToGene1>0.5</Gene2_MaxRfToGene1>
	<PhenotypeInteraction>1.0</PhenotypeInteraction>
</root>

